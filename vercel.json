{
  "redirects": [
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*",
      "permanent": false
    },
    {
      "source": "/@vite/:path*",
      "destination": "/@vite/:path*",
      "permanent": false
    },
    {
      "source": "/:invitation_label/:invitation_name",
      "destination": "https://iattend.events/:invitation_label/:invitation_name",
      "permanent": false
    },
    {
      "source": "/side-event/:side_event_id",
      "destination": "https://iattend.events/side-event/:side_event_id",
      "permanent": false
    }
  ]
}
